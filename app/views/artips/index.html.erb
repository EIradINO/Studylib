<div class="white_container">
  <h1>Artips</h1>

  <table>
    <thead>
      <tr>
        <th>Content</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @artips.each do |artip| %>
        <tr>
          <%= render 'artip', artip: artip %>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New Artip', new_artip_path %>
</div>
